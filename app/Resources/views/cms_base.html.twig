        {% set messages = app.session.flashbag.get("message") %}
        {% set titles = app.session.flashbag.get("title") %}

        {% if (messages is not empty) %}

            <script type="text/javascript">
                $(document).ready(function(){
                    $.gritter.add({
                        {% for title in titles  %}
                            title: '{{ title }}',
                        {% endfor %}

                        {% for message in messages  %}
